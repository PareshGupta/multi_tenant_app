<!DOCTYPE html>
<html>
<head>
  <title>MultiTenantApp</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

  <% if user_signed_in? %>
    <nav>
      <%= link_to 'Sign Out', destroy_user_session_path, method: :delete %>
    </nav>
  <% end %>

  <%= yield %>

</body>
</html>
